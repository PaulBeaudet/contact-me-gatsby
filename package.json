{
  "name": "contact-me",
  "private": true,
  "description": "Landing page for contacting the host of the website",
  "version": "1.0.18",
  "author": "Paul Beaudet",
  "dependencies": {
    "gatsby": "^2.27.0",
    "gatsby-cli": "^2.12.50",
    "gatsby-plugin-manifest": "^2.4.5",
    "gatsby-plugin-offline": "^3.2.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.4",
    "gatsby-source-filesystem": "^2.3.4",
    "gatsby-transformer-sharp": "^2.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-hook-form": "^5.7.2"
  },
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "cypress": "^5.6.0",
    "eslint": "^7.9.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "./build.sh",
    "clean": "gatsby clean",
    "install-globals": "npm install -g pm2 gatsby-cli serverless",
    "cypress": ". ./build.sh && npx cypress open",
    "cypress-depends": "sudo apt-get install libgtk2.0-0 libgtk-3-0 libgbm-dev libnotify-dev libgconf-2-4 libnss3 libxss1 libasound2 libxtst6 xauth xvfb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulBeaudet/contact-me-gatsby"
  },
  "bugs": {
    "url": "https://github.com/PaulBeaudet/contact-me-gatsby/issues"
  }
